{
  "gitbook": ">=3.2.2",
  "title": "FNX Documentation",
  "plugins": [
    "edit-link", "github", "prism", "-highlight", "hide-published-with", "-fontsettings", "-sharing"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/fnxjs/fnx"
    },
    "edit-link": {
